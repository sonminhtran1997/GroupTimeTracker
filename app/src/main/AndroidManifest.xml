<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leole.timetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimeSheetActivity"
            android:label="@string/title_activity_time_sheet"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SetTimeActivity"
            android:label="@string/title_activity_set_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NewEmployeeActivity"
            android:label="@string/title_activity_new_employee"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EmployeeListActivity"
            android:label="@string/title_activity_employee_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EmployeeInformationActivity"
            android:label="@string/title_activity_employee_information"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditEmployeeInformationActivity"
            android:label="@string/title_activity_edit_employee_information"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShiftInfoActivity"
            android:label="@string/title_activity_shift_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NewTimeSheet"
            android:label="@string/title_activity_new_time_sheet"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShiftList"
            android:label="@string/title_activity_shift_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>